{
  "mcpServers": {
    "codanna": {
      "command": "codanna",
      "args": ["serve", "--watch"],
      "env": null
    },
    "foundry": {
      "command": "foundry",
      "args": ["serve"],
      "env": {
        "FOUNDRY_LOG_LEVEL": "info"
      }
    },
    "graphite": {
      "command": "gt",
      "args": ["mcp"]
    }
  }
}
