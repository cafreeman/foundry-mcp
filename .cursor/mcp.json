{
  "mcpServers": {
    "codanna": {
      "command": "codanna",
      "args": ["serve", "--watch"],
      "env": null
    },
    "foundry": {
      "command": "foundry",
      "args": ["serve"],
      "env": {
        "NO_COLOR": "1",
        "RUST_LOG_STYLE": "never",
        "RUST_LOG": "error"
      }
    }
  }
}
